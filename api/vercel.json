{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "vercel-sapper"      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "PATCH",
          "DELETE"
      ]
      }
    ]
  }